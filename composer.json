{
    "name": "terminal42/contao-pageimage",
    "description": "[Contao CMS] Assign images to pages and output them on layout level using a frontend module.",
    "keywords": ["contao", "page", "image", "background"],
    "homepage": "https://github.com/terminal42/contao-pageimage",
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-pageimage/issues",
        "source": "https://github.com/terminal42/contao-pageimage",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "doctrine/dbal": "^3.3",
        "symfony/config": "^5 || ^6 || ^7",
        "symfony/dependency-injection": "^5 || ^6 || ^7",
        "symfony/http-foundation": "^5 || ^6 || ^7",
        "symfony/http-kernel": "^5 || ^6 || ^7"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "terminal42/contao-build-tools": "dev-main"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\PageimageBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\PageimageBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": false,
            "php-http/discovery": false,
            "contao/manager-plugin": false,
            "terminal42/contao-build-tools": true
        }
    }
}
